{
  "config": {
    "platform-check": false,
    "allow-plugins": {
      "kylekatarnls/update-helper": false
    },
    "audit": {
      "block-insecure": false
    }
  },
  "name": "freescout/safe-email-notifications",
  "version": "1.0.0",
  "description": "Replace FreeScout user notification emails with minimal transactional messages",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "illuminate/support": "5.5.*",
    "tormjens/eventy": "^0.5"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.28",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Freescout\\SafeEmailNotifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freescout\\SafeEmailNotifications\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Freescout\\SafeEmailNotifications\\SafeEmailNotificationsServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
